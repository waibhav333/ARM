     area     appcode, CODE, READONLY
     export __main
	 ENTRY 
__main  function		 
       
	   
       mov r7,#2	   

       vldr.f32  s1, =0.9 ; xvlaue
	   vmov.f32  s3,s1 
	   vldr.f32  s4, =1
	   vldr.f32  s2, =1	 
	   mov r4,#1
loop   cmp r4,#20   
	   beq endloop
	   vmul.f32 s3,s1,s3
	   vadd.f32 s4,s4,s2
	   vdiv.f32 s5, s3, s4
	   
	   udiv r0, r4, r7    
       mul r1, r0, r7  
       sub r2, r4, r1 
       and r10,r4,#1
	   cmp r2,#0
	   bne odd
	   beq  even
even   vadd.f32 s7,s7,s5
       b over
odd	   vsub.f32 s6,s6,s5 	    

over	
   	   add r4,r4,#1
	   b loop
	 
endloop 
       vadd.f32 s8,s6,s7
	   vadd.f32 s8,s8,s1
stop b stop
endfunc
     end  
