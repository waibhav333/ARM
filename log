vldr.f32  s1, =10   ; xvlaue
	   vldr.f32  s3, =10 ; xvalue
	   vldr.f32  s4, =1
	   vldr.f32  s2, =1	 
	   mov r4,#1
loop   cmp r4,#10   
	   beq endloop
	   vmul.f32 s3,s1,s3
	   vadd.f32 s4,s4,s2
	   vdiv.f32 s5, s3, s4
	   and r10,r4,#1
	   cmp r10,#0
	   ITE EQ 
	   vsubeq.f32 s6,s6,s5  
	   vaddne.f32 s7,s7,s5 
	   b loop
     vadd.f32 s1,s6,s7
endloop
